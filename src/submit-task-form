export function submitTaskForm() {
	const taskTitleInput = document.querySelector('.task-title-input');
	const taskDetailsInput = document.querySelector('.task-details-input');
	const taskDateInput = document.querySelector('.task-date-input');

	let title = taskTitleInput.value;
	let details = taskDetailsInput.value;
	let date = taskDateInput.value;

	return { title, details, date };
}
